@model ClassesMarmitex.HorarioEntrega

<div class="profile-content-backfull">
    <h2>Adicionar horário de entrega</h2>
    <p style="font:italic;" class="vermelho-marmitex">os campos com * são de preenchimento obrigatório</p>
    <div class="col-md-10">

        <form method="post" action="@Session["urlBase"]/HorarioEntrega/AdicionarHorario">
            @Html.Partial("_FormHorarioEntrega")

            <div class="col-md-12">
                <div class="buttons-formcontrol">
                    <input type="submit" class="btn btn-success" value="adicionar" />
                </div>
            </div>

        </form>

    </div>
</div>
