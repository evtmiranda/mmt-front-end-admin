@model ClassesMarmitex.UsuarioLoja

<div class="profile-content-backfull">
    <h2>Adicionar usuário</h2>
    <p style="font:italic;" class="vermelho-marmitex">os campos com * são de preenchimento obrigatório</p>
    <div class="col-md-10">
        <form method="post" action="@Session["urlBase"]/Usuario/AdicionarUsuario">
            @Html.Partial("_FormUsuario")

            <div class="col-md-12">
                <div class="buttons-formcontrol">
                    <input type="submit" class="btn btn-success" value="adicionar usuario" />
                </div>
            </div>
        </form>

    </div>
</div>
